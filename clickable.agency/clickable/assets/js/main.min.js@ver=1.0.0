        for(let i = 0; i < this.length; i++){
            cb(this[i], i, this);
        }
    }
}

if(!NodeList.prototype.reduce){
    NodeList.prototype.reduce = function (cb, init) {
        let result = init || 0;
        for(let i = 0; i < this.length; i++){
            result = cb(result, this[i], i);
        }
        return result;
    };
}

