$(window).load(function(){
	$('.slider-list.flexslider').flexslider({
		directionNav: false,
		animation: "fade",
		start: function(slider){
			var count_li = 0;
			$('.slider-list .slides li').each(function(indx, element){
				if (!$(element).hasClass('clone')) {
					$('.slider-list .flex-control-nav li').eq(count_li).find("a").text($(element).find("img").data('nameslide'));
					count_li = count_li + 1;
				}
			});
        }
	});
});